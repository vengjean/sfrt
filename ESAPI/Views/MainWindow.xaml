<Window x:Class="SFRT_PlanningScript.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SFRT_PlanningScript.Views"
        mc:Ignorable="d"
        Title="SFRT_PlanningScript"
        Height="880"
        Width="620">
    <DockPanel LastChildFill="False">
        <Grid DockPanel.Dock="Top">
            <Grid.RowDefinitions>
                <RowDefinition Height="*"></RowDefinition>
                <RowDefinition Height="Auto"></RowDefinition>
            </Grid.RowDefinitions>
            <TabControl>
                <TabItem Name="SphereLatticeTab"
                         Header="SphereLattice"/>
                <!-- <TabItem Name="GridTab" -->
                <!-- Header="Grid"/> -->
                <!-- <TabItem Name="RapidRodsTab"
                         Header="RapidRods"/> -->
                <!-- <TabItem Name="GridMLCTab"
                         Header="GridMLC"/> -->
            </TabControl>
        </Grid>
    </DockPanel>
</Window>
